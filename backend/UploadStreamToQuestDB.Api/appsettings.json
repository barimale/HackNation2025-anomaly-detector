{
  "RabbitMQ": {
    "HostName": "localhost",
    "ConsumerDispatchConcurrency": 1
  },
  "ConnectionStrings": {
    "Database": "Data Source=localhost,1500;Database=DNNdb;Uid=sa;Password=Password_123#; TrustServerCertificate=True",
    "OracleDB": "Data Source = localhost:1521/FREE; User ID = SYS; Password = Password_123#; DBA Privilege = SYSDBA"
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Trace"
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/upload-stream-to-questdb-.txt",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "upload-stream-to-questdb"
    }
  },
  "ReadQuestDbAddress": "http://127.0.0.1",
  "QuestDbAddress": "127.0.0.1",
  "QuestDbPort": "9000",
  "QuestDbSettings": "username=admin;password=quest;auto_flush=on;auto_flush_rows=80000;",
  "AllowedExtension": "txt",
  "AntivirusActive": true
}
